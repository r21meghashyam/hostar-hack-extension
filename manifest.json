{
    "manifest_version": 2,
    "name": "Hotstar Hack",
    "version": "1.0.0",
    "description": "Hack to watch hotstar unlimited",
    "default_locale": "en",
    "icons": {
        "16": "favicon-16x16.png",
        "32": "favicon-32x32.png"
    },
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js":["content-script.js"]
        }
    ],
    "permissions": [
        "activeTab",
        "storage",
        "https://www.hotstar.com/*"
      ]
}